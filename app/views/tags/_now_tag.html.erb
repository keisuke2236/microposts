<div style="font-size:1.5em; text-align:center;">登録タグの削除<div>
<% for i in 0..100 %>
    <% break if @tags[i]==nil %>
    <% #link_to @tags[i].name, tags_show_path(id: @tags[i].id) %>
    <%= link_to @tags[i].name ,tags_destroy_path(:del_id => @tags[i].id), :method => :delete , :class => 'btn btn-danger col-xs-12 col-lg-4  col-sm-6',:style=>"margin-top:2px;", data: {confirm: "#{@tags[i].name}を本当に削除しますか？"} %>
<% end %>
